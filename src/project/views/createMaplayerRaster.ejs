<div class="modal fade" id="displayModalToCreateMaplayerRaster" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-primary" role="document">
        <div class="modal-content">
            
                    <div class="card cardmodal">
                        <div class="card-header">
                            <strong><i class="fa fa-plus"></i> CREATE LAYER RASTER</strong>
                            <!-- <small>Form</small> -->
                        </div>
                        <form id="formcreateMaplayerRaster" enctype="multipart/form-data">
                            <div class="card-body">
                                <div class="form-group">
                                    <label for="name-imput">NAME</label>
                                    <input id="maplayer-raster-name" minlength=5 type="text" class="form-control" name="name" placeholder="Insert name layer raster" required>
                                </div>
                                <div class="form-group">
                                    <label for="maplayer-raster-description">DESCRIPTION</label>
                                    <textarea id="maplayer-raster-description" name="description-input" rows="9" class="form-control" placeholder="Insert description..."></textarea>
                                </div>
                                <div class="form-group">
                                    <label for="maplayer-raster-file">RASTER FILE</label>
                                    <br>
                                    <input id="maplayer-raster-file" type="file" name="file-input-raster">
                                </div>
                                <!-- <div id="errorCreateMaplayerVector" class="alert alert-danger" role="alert" style="display: none;">
                                        Ingrese todos los campos
                                    </div> -->
                                    <div class="modal-footer">
                                        <button type="button" class="btn btn-secondary" data-dismiss="modal" id="MaplayerRastercancel">CANCEL <i class="fa fa-times"></i></button>
                                        <button type="button" class="btn btn-primary" id="createMaplayerRaster">CREATE <img src="img/logo.png" width="15%" alt=""></i></button>
                                    </div>
                            </div>
                        </form>
                        <div class="progress">
                            <div class="progress-bar progress-bar-striped progress-bar-animated" role="progressbar" aria-valuenow="0" aria-valuemin="0" aria-valuemax="100"></div>
                        </div>
                    </div>
            <!-- /.modal-content -->
        </div>
    </div>
</div>